{
  "name": "beme-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "start:consumer": "node dist/workers/event-consumer.js",
    "start:voice": "node dist/workers/voice-worker.js",
    "start:money": "node money-service.js",
    "start:schedule": "node schedule-service.js",
    "start:body": "node body-service.js",
    "start:energy": "node energy-service.js",
    "start:goals": "node goals-service.js",
    "dev": "tsx watch index.ts",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "import:foods": "node scripts/importFoundationFoods.js",
    "remove:non-foundation-foods": "node scripts/removeNonFoundationFoods.js",
    "seed:popular-foods": "node scripts/seedPopularFoods.js",
    "migrate:up": "node-pg-migrate up",
    "migrate:create": "node-pg-migrate create"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.998.0",
    "@aws-sdk/client-sqs": "^3.998.0",
    "@aws-sdk/s3-request-presigner": "^3.1000.0",
    "@google/genai": "^1.41.0",
    "@google/generative-ai": "^0.21.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.6.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.13.0",
    "pino": "^10.3.1",
    "rate-limit-redis": "^4.3.1",
    "redis": "^4.7.0",
    "resend": "^6.9.2",
    "wave-resampler": "^1.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.161",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.3",
    "@vitest/coverage-v8": "^4.0.18",
    "supertest": "^7.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
